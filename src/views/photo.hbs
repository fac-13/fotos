<section>
    {{#each data}}
    <h1>{{this.title}}</h1>
    <img src={{this.image_url}} alt="{{this.title}}" />
    <p>Description: {{this.description}}</p>

    <p>Posted by:
        <a href="/profile/{{this.username}}" alt={{this.username}}>{{this.username}}</a>
    </p>
    <p>Date posted: {{this.date}}</p>
    {{/each}}
</section>